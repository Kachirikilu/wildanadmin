.ul-top3W {
    position: fixed;
    width: 100%;
    z-index: 3;
    overflow-y: auto;
    overflow-x: hidden;
}
#center {
    display: flex;
    justify-content: center;
    align-items: center;
    vertical-align: center;
    width: 100%;
    height: 100%;
}
#container-Wallpaper {
    display: grid;
    background: rgba(64, 64, 64, 0.64);
    border-radius: 0.8em;
    padding: 10px;
    justify-content: center;
    align-items: center;
    gap: 12px 12px;
}


@keyframes ul-top3W {
    0%{
        transform: scale(0.24);
        transform-origin: 180% 50%;
    }
    68%{
        transform-origin: -2% 50%;
    }
    100%{
        transform: scale(1);
    }
}
@keyframes ul-top3WR {
    0%{
        transform: scale(1);
    }
    20%{
        transform-origin: -2% 50%;
    }
    100%{
        transform: scale(0.24);
        transform-origin: 180% 50%;
    }
}
@keyframes ul-top3WR2 {
    0%{
        transform: scale(1);
    }
    20%{
        transform-origin: -2% 50%;
    }
    100%{
        transform: scale(0.48);
        transform-origin: 50% -200%;
    }
}

.w-slide {
    background: #202121ba;
    border-radius: 0.8em;
    color: rgb(230, 230, 230);
    border: 0px;
    aspect-ratio: 3 / 1.8;
    transition: 0.21s ease-out;
}
.w-sliderx {
    border-radius: 16px;
    transition: 0.21s ease-out;
}

.w-slide:hover {
    background: #ea2009fb;
    animation: wallpaperOpsiKey 0.21s ease forwards;
}
@keyframes wallpaperOpsiKey {
    0%{
        transform: scale(1);
    }
    42%{
        transform: scale(1.02);
    }
    100%{
        transform: scale(0.98);
    }
}
.w-slide:active {
    background: #cc0f09fb;
    animation: wallpaperOpsiKeyA 0.06s ease forwards
}
@keyframes wallpaperOpsiKeyA {
    0%{
        transform: scale(1.04);
    }
    64%{
        transform: scale(0.972);
    }
    100%{
        transform: scale(0.96);
    }
}

function w1() {
    document.getElementById("background").style.backgroundImage = "url(/FileWeb/w1.jpg)";
    document.getElementById("background").style.backgroundSize = "100%"
    document.getElementById("background").style.backgroundRepeat = "repeat"
    document.getElementById("background").style.backgroundAttachment = "fixed"
  
    let wallpaper = document.getElementById("ul-top3W")
    if (wallpaper.style.display == "none") {
    }
    else {
        setTimeout(out, 340);
        wallpaper.style.animation = "ul-top3WR2 340ms ease forwards"
        function out() {
            wallpaper.style.display = "none"
        }
    }
    
    let blur = document.getElementById("blur")
    if (blur.style.filter == "none") {
    }
    else {
        setTimeout(out, 500);
        blur.style.animation = "blurR 500ms ease forwards";
        function out() {
            blur.style.filter = "none";
        }
    }
}


