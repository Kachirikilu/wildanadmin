let wallpaperClose = document.getElementById("ul-top3W")
if (wallpaperClose.style.display == "inline") {
    setTimeout(out, 260);
    wallpaperClose.style.animation = "ul-top3WR2 460ms ease forwards"
    function out() {
        wallpaperClose.style.display = "none"
    }
}

