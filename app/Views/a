.UltraBlurPro {
    position: absolute;
    /* animation: UltraBlurPro 1.2s ease forwards; */
    position: absolute;
    width: 100%;
    height: 100%;
    padding-bottom: 26%;
    z-index:-1;
    overflow: hidden;
}

/* @keyframes UltraBlurPro {
    0%{
        filter: blur(6px);
        transform: translateY(-100%) scale(0);
    }
    40%{
        filter: blur(12px);
        transform: translateY(6%) scale(1.1);
    }
    60%{
        transform: translateY(0%) scale(1);
    }
    100% {
        filter: blur(0px);
    }
} */

function w1() {
    document.getElementById("background").style.backgroundImage = "url(/FileWeb/w1.jpg)";
    document.getElementById("background").style.backgroundSize = "100%"
    document.getElementById("background").style.backgroundRepeat = "repeat"
    document.getElementById("background").style.backgroundAttachment = "fixed"
  
    let wallpaper = document.getElementById("ul-top3W")
    if (wallpaper.style.display == "none") {
    }
    else {
        setTimeout(out, 340);
        wallpaper.style.animation = "ul-top3WR2 340ms ease forwards"
        function out() {
            wallpaper.style.display = "none"
        }
    }
    
    let blur = document.getElementById("blur")
    if (blur.style.filter == "none") {
    }
    else {
        setTimeout(out, 500);
        blur.style.animation = "blurR 500ms ease forwards";
        function out() {
            blur.style.filter = "none";
        }
    }
}


